<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="238.0" prefWidth="312.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.programmingpatternsproject.ChangeDateViewController">
   <children>
      <DatePicker fx:id="datePicker" layoutX="69.0" layoutY="107.0" />
      <Button fx:id="cancelButton" layoutX="14.0" layoutY="199.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="239.0" layoutY="199.0" mnemonicParsing="false" onAction="#handleConfirmButtonAction" text="Confirm" />
      <Label layoutX="122.0" layoutY="47.0" text="Change Date" />
   </children>
</AnchorPane>
